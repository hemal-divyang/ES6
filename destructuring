//Destructuring is a feature in JavaScript that allows you to extract values from arrays or properties from objects
and assign them to variables.
//Example

// ARRAY destructuring
let numbers = [1, 2, 3];
let [a, b, c] = numbers;

console.log(a); // Output: 1
console.log(b); // Output: 2
console.log(c); // Output: 3

// OBJECT destructuring
let person = { name: 'Alice', age: 30, city: 'Wonderland' };

let { name, age, city } = person;

console.log(name); // Output: Alice
console.log(age);  // Output: 30
console.log(city); // Output: Wonderland

// FUNCTION  parameter destructuring
function printPersonDetails({ name, age, city }) {
  console.log(`${name} is ${age} years old and lives in ${city}.`);
}

let person = { name: 'Alice', age: 28, city: 'Wonderland' };
printPersonDetails(person);
// Output: Alice is 28 years old and lives in Wonderland.


